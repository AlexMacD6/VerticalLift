	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"seed": "npx prisma db seed",
		"db:studio": "dotenv -e .env -- npx prisma studio",
		"db:studio:local": "dotenv -e .env -- npx prisma studio",
		"db:studio:prod": "dotenv -e .env.production -- npx prisma studio",
		"db:reset": "npx prisma migrate reset",
		"db:start": "docker compose up -d",
		"db:start-legacy": "docker start rigreview-db-1",
		"db:stop": "docker compose down",
		"db:migrate": "npx prisma migrate dev",
		"db:deploy": "npx prisma migrate deploy",
		"postinstall": "npx prisma generate",
		"db:fork": "setenv DATABASE_URL \"$(neon branches create --parent main --name $(git rev-parse --abbrev-ref HEAD) --output json | jq -r '.connection_uris[0].connection_uri')\"",
		"db:list": "neon branches list",
		"auth:clear": "tsx scripts/clear-auth-sessions.ts",
		"migrate:assets": "tsx scripts/migrate-assets-to-operator-domains.ts"
	},
